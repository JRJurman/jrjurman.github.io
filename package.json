{
  "name": "jrjurman.github.io",
  "version": "1.0.0",
  "description": "my website",
  "main": "index.js",
  "scripts": {
    "prebuild": "mkdir dist/js/ -p; cp src/index.html dist/.",
    "build": "browserify src/js/main.jsx -t [ babelify --presets [ es2015 react ] ] -o dist/js/main.js",
    "postbuild": "echo 'build finished'",
    "prestart": "npm run build",
    "start": "node webserver.js",
    "watch": "onchange \"src/**/*\" -- npm run build",
    "dev": "shell-exec --colored-output \"npm run watch\" \"npm run start\"",
    "deploy": "gh-pages -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JRJurman/jrjurman.github.io.git"
  },
  "author": "Jesse Jurman",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JRJurman/jrjurman.github.io/issues"
  },
  "homepage": "https://github.com/JRJurman/jrjurman.github.io#readme",
  "dependencies": {
    "babel": "^6.5.2",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "cash": "^0.8.0",
    "express": "^4.13.4",
    "moviedb": "^0.2.3",
    "onchange": "^2.2.0",
    "react-bootstrap": "^0.28.5",
    "react-dom": "^0.14.8",
    "react-router": "^2.0.1",
    "shell-executor": "^0.3.2"
  },
  "browserify": {
    "extension": [
      "jsx"
    ],
    "transform": [
      "babelify"
    ]
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0"
  }
}
