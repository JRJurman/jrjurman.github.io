{
  "name": "jrjurman.github.io",
  "version": "1.0.0",
  "description": "my website",
  "main": "index.js",
  "scripts": {
    "prebuild:css": "mkdir -p dist/css/",
    "build:css": "cp src/css/*.css dist/css/.",
    "prebuild:html": "mkdir -p dist/about dist/projects",
    "build:html": "echo dist/ dist/about/ dist/projects/ | xargs -n 1 cp src/index.html",
    "prebuild:jsx": "mkdir -p dist/js/",
    "build:jsx": "browserify src/js/main.jsx -t [ babelify --presets [ es2015 react ] ] -o dist/js/main.js",
    "build": "shell-exec --colored-output \"npm run build:html\" \"npm run build:css\" \"npm run build:jsx\"",
    "postbuild": "echo 'build finished'",
    "prestart": "npm run clean; npm run build",
    "start": "node webserver.js",
    "watch": "onchange \"src/**/*\" -- npm run build",
    "preclean": "mkdir -p dist/garbage",
    "clean": "rm -r dist/*",
    "dev": "shell-exec --colored-output \"npm run watch\" \"npm run start\"",
    "predeploy": "npm run build; cp CNAME dist/.",
    "deploy": "gh-pages -d dist -b master -m \"automatic commit from gh-pages\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JRJurman/jrjurman.github.io.git"
  },
  "author": "Jesse Jurman",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JRJurman/jrjurman.github.io/issues"
  },
  "homepage": "https://github.com/JRJurman/jrjurman.github.io#readme",
  "dependencies": {
    "babel": "^6.5.2",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "cash": "^0.8.0",
    "express": "^4.13.4",
    "moviedb": "^0.2.3",
    "onchange": "^2.2.0",
    "react-bootstrap": "^0.28.5",
    "react-dom": "^0.14.8",
    "react-router": "^2.0.1",
    "shell-executor": "^0.3.2"
  },
  "browserify": {
    "extension": [
      "jsx"
    ],
    "transform": [
      "babelify"
    ]
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "gh-pages": "^0.11.0"
  }
}
